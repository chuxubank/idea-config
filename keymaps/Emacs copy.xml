    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="ActivateNotificationsToolWindow">
    <keyboard-shortcut first-keystroke="meta alt n" />
  </action>
  <action id="ActivatescrcpyToolWindow">
    <keyboard-shortcut first-keystroke="meta alt s" />
  </action>
  <action id="Logcat.ToggleViewFormat" />
  <action id="PreviousProjectWindow">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="shift ctrl o" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl meta e" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="shift ctrl meta e" />
  </action>
</keymap>